# DeepSeek API配置
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1
DEEPSEEK_MODEL=deepseek-chat
DEEPSEEK_TIMEOUT=60

# 重试配置
MAX_RETRIES=3
RETRY_DELAY=1.0

# 日志配置
LOG_LEVEL=INFO
LOG_FILE_ENABLED=true
LOG_MAX_BYTES=10485760
LOG_BACKUP_COUNT=3

# 应用配置
APP_NAME=system-diagram-agent
APP_VERSION=1.0.0
APP_DEBUG=false

# Web服务配置
WEB_HOST=localhost
WEB_PORT=5000
WEB_DEBUG=false

# 控制器配置
MAX_CONCURRENT_SESSIONS=10
SESSION_TIMEOUT=3600
ENABLE_CACHING=true
CACHE_TTL=300

# 代理配置
REQUIREMENT_AGENT_TEMPERATURE=0.1
SYSTEM_AGENT_TEMPERATURE=0.3
DIAGRAM_AGENT_TEMPERATURE=0.2
QUALITY_AGENT_TEMPERATURE=0.1
INTERACTION_AGENT_TEMPERATURE=0.7

# 质量检查配置
QUALITY_THRESHOLD=75
AUTO_FIX_ENABLED=true
MAX_FIX_ATTEMPTS=3

# 性能配置
ENABLE_STREAMING=true
BATCH_SIZE=5
REQUEST_TIMEOUT=120 